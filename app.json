{
  "expo": {
    "name": "planck",
    "slug": "planck",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/images/logo.png",
    "splash": {
        "image": "./src/images/logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
